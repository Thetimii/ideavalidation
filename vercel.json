{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/generate-spec/**/*.ts": {
      "maxDuration": 25
    },
    "src/app/api/generate-async/**/*.ts": {
      "maxDuration": 300
    },
    "src/app/api/jobs/**/*.ts": {
      "maxDuration": 10
    },
    "src/app/api/custom-domains/**/*.ts": {
      "maxDuration": 30
    }
  }
}
